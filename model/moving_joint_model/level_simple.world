        <child>level01_support</child>
        <pose>0 0 4.1 0 0 0</pose>
      </joint>

      <joint name="joint_level02_support" type="fixed">
        <parent>base</parent>
        <child>level02_support</child>
        <pose>0 0 7.1 0 0 0</pose>
